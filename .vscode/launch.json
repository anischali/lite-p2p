{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      //"args": ["stun:stun.sipnet.net", "3478", "36896", "41173"],
      "args": ["34.203.251.243", "3478", "36896", "41173", "127.0.0.1"],
      //"args": ["2001:4860:4864:5:8000::1", "19302", "5001", "5002", "::"],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "${workspaceFolder}/build/",
      "program": "${workspaceFolder}/build/stun-client6",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}