{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["stun:stun.l.google.com", "19302", "36896", "41173"],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "${workspaceFolder}/build/",
      "program": "${workspaceFolder}/build/stun-client",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}